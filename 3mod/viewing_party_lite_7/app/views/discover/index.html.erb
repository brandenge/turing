<h3>Discover Movies</h3>
<%= link_to "Top Rated Movies", user_dashboard_movies_path(@user, q: 'top rated'), method: :get %>
<%= form_with url: user_dashboard_movies_path(@user, q: @search), method: :get, local: true do |form| %>
  <%= form.search_field :q, placeholder: "Search" %>
  <%= form.submit "Find Movies" %>
<% end %>
