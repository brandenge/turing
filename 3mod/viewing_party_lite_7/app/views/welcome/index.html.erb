<% if current_user %>
  <%= button_to 'Log Out', logout_path, method: :delete %>
<% else %>
  <%= form_with model: @user, url: new_user_path, method: :get, local: true do |form| %>
    <%= form.button 'Create New User' %>
  <% end %>
  <%= button_to 'Log In', login_form_path, method: :get %>
<% end %>

<% if current_user %>
  <h2>Existing Users</h2>

  <ul>
    <% @users.each do |user|%>
      <li><%= user.email %></li>
    <% end %>
  </ul>
<% end %>
