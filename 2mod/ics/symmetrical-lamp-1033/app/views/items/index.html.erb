<ul>
  <h2>Items Index</h2>
<% @items.each do |item| %>
  <ul>
    <h3>Name: <%= item.name %></h3>
    <p>ID: <%= item.id %></p>
    <p>Price: <%= format_price(item) %></p>
    <p>Supermarket: <%= item.supermarket.name %></p>
    <p>Customer Count: <%= item.customer_count %></p>
  </ul>
<% end %>
</ul>
